{
    "PACKAGE":                    "KayaRunnerSDK",
    "DESCRIPTION":                "Kaya Strategy Module runner. Private slice of KMP SDK.",
    "VERSION":                    "RWM",
    "VERSION_NO":                 "1.0",
    "DEFAULT": {
         "conf-file":             "conf/kaya_runner.conf.json",
         "log-file":              "log/kaya_runner.log",
         "host":                  "0.0.0.0",
         "port":                  8080,
         "base-path":             "/",
         "workers":               4,
         "debug-flag":            false
    },
    "ENV": {
        "module-name":            "KAYA_MODULE_NAME",
        "module-version":         "KAYA_MODULE_VERSION"
    }
}
